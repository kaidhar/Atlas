<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.GaussianBlur?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dataCheck.CredentialsController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Credentials" />
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="91.0" prefWidth="491.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="UserName" layoutX="81.0" layoutY="61.0" />
            <TextField fx:id="Password" layoutX="81.0" layoutY="109.0">
               <effect>
                  <GaussianBlur />
               </effect></TextField>
            <Button fx:id="Submit" layoutX="189.0" layoutY="198.0" mnemonicParsing="false" onAction="#updateCredentials" text="Submit" />
            <Label layoutX="42.0" layoutY="66.0" text="User" />
            <Label layoutX="12.0" layoutY="114.0" text="Password" />
         </children>
    </AnchorPane>
  </children>
</VBox>
